	<string name="auth_state_failed">!!!Authentication Failed!!!</string>
	<string name="auth_state_none">Not Authenticated</string>
	<string name="auth_state_voice">Voice Authenticated</string>
	<string name="auth_state_full">Authenticated</string>
	<string name="auth_begin_button">Begin Authentication</string>

	<!-- Auth Wizard -->
	<string name="auth_intro_title">Voice Authentication</string>
	<string name="auth_intro">To begin, please ask
		your conversational partner to press the \"Begin Authentication\"
		button if they have not already done so.\n\n

		On the next screens, you will be shown a series of words or
		symbols. Some you will need to read out to your partner, and
		some your partner will read out to you. You only need to verify
		that what your partner reads to you matches what you are
		shown.\n\n

		You can select what set of words or symbols you would like to
		use from the dropdown below. Once you and your partner have
		agreed on a set, press \"Next\" to continue.</string>
	<string name="auth_cancel_button">Authenticate Later</string>
	<string name="auth_next_button">Next</string>
	<string name="auth_no_generator">No list generator selected!</string>
	<string name="auth_symbol_num">Symbol group %d</string>
	<string name="auth_attack_probability">Attack probability: %f</string>
	<string name="auth_your_symbol_title">Read these out to your partner:</string>
	<string name="auth_their_symbol_title">Your partner will read these out to you:</string>

	<string name="auth_query_yours">Did all your symbols match your partner\'s?</string>
	<string name="auth_query_theirs">Did all your partner\'s symbols match yours?</string>
	<string name="auth_yes_button">Yes</string>
	<string name="auth_no_button">No</string>
</resources>
